                        <small><a href="garden-category.html" title=""><i class="fa fa-eye"></i>@item.ViewingCount</a></small>
                        <small><a href="garden-single.html" title="">@item.CreatedDate</a></small>
                        <small><a href="#" title="">by @item.User.FirstName @item.User.LastName</a></small>
